{
    "name": "cristianomzn/contatos",
    "description": "Modern contact management system with MVC architecture",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/console": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/config": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/asset": "^7.0",
        "symfony/form": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/ux-twig-component": "^2.0",
        "symfony/ux-live-component": "^2.0",
        "symfony/ux-turbo": "^2.0",
        "kreait/firebase-php": "^7.0",
        "google/cloud-firestore": "^1.40",
        "google/cloud-storage": "^1.35",
        "google/cloud-secret-manager": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true,
            "php-http/discovery": true
        }
    }
}
